    "autoprefixer": "^10.4.13",
    "babel-jest": "^29.7.0",
    "eslint": "^8.0.1",
    "eslint-plugin-react": "^7.31.11",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.18",
    "prettier": "2.8.0",
    "tailwindcss": "^3.2.1",
    "ts-jest": "^29.2.5",
    "typescript": "^4.9.5",
    "vite": "^4.5.9",
    "vite-plugin-pwa": "^0.14.3"
